{
    "name": "xms/bootstrap-template",
    "type": "typo3-cms-extension",
    "description": "A Bootstrap 5 based template extension for TYPO3 providing responsive layouts and customizable design options",
    "authors": [
        {
            "name": "Peter Pan",
            "email": "pp@n-sight.de",
            "role": "Developer"
        }
    ],
    "license": ["GPL-2.0-or-later"],
    "require": {
        "typo3/cms-core": "^11.5",
        "typo3/cms-fluid-styled-content": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "TYPO3\\BootstrapTemplate\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "bootstrap_template"
        }
    }
}
